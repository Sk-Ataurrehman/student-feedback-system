<!DOCTYPE html>
<html lang="en">

<head>
  <title>Students Feedbacks (Staff) - FeedbackHUT</title>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="/images/favicon.png" rel="icon">
  <link href="/images/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link href="https://fonts.gstatic.com" rel="preconnect">
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="/vendor/quill/quill.snow.css" rel="stylesheet">
  <link href="/vendor/quill/quill.bubble.css" rel="stylesheet">
  <link href="/vendor/remixicon/remixicon.css" rel="stylesheet">
  <link href="/vendor/simple-datatables/style.css" rel="stylesheet">

  <!-- Template Main CSS File -->
  <link href="/stylesheets/style.css" rel="stylesheet">

</head>

<body>

  <!-- ======= Header ======= -->
  <%-include('../components/header')%>
  <!-- End Header -->

  <!-- ======= Sidebar ======= -->
  <%-include('../components/navbar')%>
  <main id="main" class="main">

    <div class="pagetitle">
      <h1>Students Feedback (Staff) (Analysis)</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/admin-feedback/dashboard">Home</a></li>
          <li class="breadcrumb-item">Feebacks</li>
          <li class="breadcrumb-item active">Students Feedbacks</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section">
      <div class="row">

        <form action="/admin-feedback/analysis/course" method="post" class="row g-3 needs-validation" novalidate>
          <div class="mb-3 col-md-3">
            <label for="acadyear" class="form-label">Academic Year</label>
            <select name="acadyear" class="form-select" required>
                <option value="" selected>Choose Academic Year</option>
                <%dates.forEach((element)=>{%>
                  <option value="<%=element%>"><%=element%></option>
                <%})%>
            </select>
            <div class="valid-feedback">Looks good!</div>
            <div class="invalid-feedback">Please select Academic Year.</div>
          </div>
          
          <div class="mb-3 col-md-3">
            <label for="department" class="form-label">Department</label>
            <select name="department" class="form-select" required>
                <option value="" selected>Choose Department</option>
                <option value="Computer Engineering">Computer Engineering</option>
                <option value="Information Technology">Information Technology</option>
                <option value="Electronics and Telecommunication">Electronics and Telecommunication</option>
                <option value="Mechanical Engineering">Mechanical Engineering</option>
                <option value="Electrical Engineering">Electrical Engineering</option>
            </select>
            <div class="valid-feedback">Looks good!</div>
            <div class="invalid-feedback">Please select Department.</div>
          </div>

          <div class="mb-3 col-md-3">
            <label for="staff" class="form-label">Staff Name</label>
            <input type="text" class="form-control" name="staff" placeholder="Staff Name" required>
            <div class="valid-feedback">Looks good!</div>
            <div class="invalid-feedback">Please enter Staff Name.</div>
          </div>

          
          <div class="col-md-3">
            <button type="submit" class="btn btn-primary" style="margin:11%">Submit</button>
          </div>  
        </form>
      </div>
    </section>
    
    <%if(noents==true){%>
      <div class="text-center">
        <h5> No Data found!</h5>
      </div>
    <%}%>  

    <div id="courseContent">

    <%if(!(avgs && Object.keys(avgs).length === 0 && Object.getPrototypeOf(avgs) === Object.prototype) && noents==false) {%>
      <div class="row g-2">   
        <div class="card">
          
        <table width="100%" class="table">
          <br>
          <thead>
            <tr>
              <th>Sr. No</th>
              <th>Particulars</th>
              <th>Average Marks</th>
          </tr>
          </thead>
          <tbody>
            
              <tr>
                  <td>1</td>
                  <td>Punctuality</td>
                  <td><%=avgs.punc%></td>
              </tr>

              <tr>
                <td>2</td>
                <td>Accessibility / Availability</td>
                <td><%=avgs.accessibility%></td>
              </tr>

              <tr>
                <td>3</td>
                <td>Sincerity</td>
                <td><%=avgs.sincerity%></td>
              </tr>
              
              <tr>
                <td>4</td>
                <td>Behaviour</td>
                <td><%=avgs.behaviour%></td>                         
              </tr>
            
              <tr>
                <td>5</td>
                <td>Subject Knowledge</td>
                <td><%=avgs.knowledge%></td>
              </tr>

              <tr>
                <td>6</td>
                <td>Full Hour Engagement of Lecture & Practical</td>
                <td><%=avgs.engagement%></td>
              </tr>

              <tr>
                <td>7</td>
                <td>Presentation Skills</td>
                <td><%=avgs.presentation%></td>                         
              </tr>

              <tr>
                <td>8</td>
                <td>Interaction with students</td>
                <td><%=avgs.interaction%></td>
              </tr>

              <tr>
                <td>9</td>
                <td>Use of Teaching aids</td>
                <td><%=avgs.aids%></td>                                                
              </tr>

              <tr>
                <td>10</td>
                <td>Completion of Syllabus</td>
                <td><%=avgs.completion%></td>                      
              </tr>

              <tr>
                <td>11</td>
                <td>Practices & Revision</td>
                <td><%=avgs.practices%></td>                       
              </tr>

              <tr>
                <td>12</td>
                <td>Test & Evolution</td>
                <td><%=avgs.evolution%></td>                        
              </tr>

              <tr>
                <td>13</td>
                <td>Ability to maintain discipline</td>
                <td><%=avgs.ability%></td>                          
              </tr>

              <tr>
                <td>14</td>
                <td>Overall perfomance</td>
                <td><%=avgs.overall%></td>                         
              </tr>
          </tbody>
        </table>
        </div>
      </div>
    <section class="section">
      <div class="row">
        
        <div class="col-lg-12">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Course Feedbacks</h5>

              <!-- Bar Chart -->
              <div id="barChart"></div>

              <script>
                document.addEventListener("DOMContentLoaded", () => {
                  new ApexCharts(document.querySelector("#barChart"), {
                    series: [{
                      name: 'Average Marks',
                      data: ['<%=avgs.punc%>', '<%=avgs.accessibility%>', '<%=avgs.sincerity%>', '<%=avgs.behaviour%>', '<%=avgs.knowledge%>', '<%=avgs.engagement%>', '<%=avgs.presentation%>', '<%=avgs.interaction%>', '<%=avgs.aids%>', '<%=avgs.completion%>','<%=avgs.practices%>', '<%=avgs.evolution%>', '<%=avgs.ability%>', '<%=avgs.overall%>']
                    }],
                    chart: {
                      type: 'bar',
                      height: 600
                    },
                    plotOptions: {
                      bar: {
                        borderRadius: 4,
                        horizontal: true,
                      }
                    },
                    dataLabels: {
                      enabled: false
                    },
                    xaxis: {
                      categories: ['Punctuality', 'Accessibility', 'Sincerity', 'Behaviour', 'Subject Knowledge', 
                      'Full Hour Engagement of Lecture and Practical', 'Presentation Skills','Interaction with Students',
                      'Use of teaching aids','Completion of Syllabus','Practices and Revision','Test and Evolution',
                      'Ability to maintain discipline','Overall Performance'
                      ],
                    }
                  }).render();
                });
              </script>
              <!-- End Bar Chart -->

              <!-- Column Chart -->
        <div id="columnChart"></div>

        <script>
          document.addEventListener("DOMContentLoaded", () => {
            var chart = new ApexCharts(document.querySelector("#columnChart"), {
              series: [{
                name: 'Yes',
                data: ['<%=freq.displayed.yes%>', '<%=freq.test.yes%>', '<%=freq.marks.yes%>', '<%=freq.curriculum.yes%>', '<%=freq.assessed.yes%>', '<%=freq.classtest.yes%>']
              }, {
                name: 'No',
                data: ['<%=freq.displayed.no%>', '<%=freq.test.no%>', '<%=freq.marks.no%>', '<%=freq.curriculum.no%>', '<%=freq.assessed.no%>', '<%=freq.classtest.no%>']
              }],
              chart: {
                type: 'bar',
                height: 600
              },
              plotOptions: {
                bar: {
                  horizontal: true,
                  columnWidth: '55%',
                  endingShape: 'rounded'
                },
              },
              dataLabels: {
                enabled: false,
              },
              stroke: {
                show: true,
                width: 2,
                colors: ['transparent']
              },
              legend:{
                fontSize: '15pt'
              },
              xaxis: {
                categories: ['Whether the academic Calendar of MBSTE was displayed before the beginning of the term?',
                              'Whether the progressive skill test/class tests are conduted as per MSBTE Calendar?',
                              'The test marks are displayed on the notice board within 15 days from the date of the test?',
                              'Was 100% Curriculum covered during the last term?',
                              'Are you being continously assessed for Laboratory work?',
                              'Do you know your class test marks at the end of the term?'],
                
              },
              fill: {
                opacity: 1,
                colors: ['#6666ff','#ff944d']
              },
            })
            chart.render();
          });

              
        </script>
                  
                  
          </div>
        </div>        
      </div>


    </section>
    <%}%>

    <div class="card">
        <div class="card-body">
                  <h5 class="card-title">Feedback Comparison Among the Staffs</h5>

                  <div id="comChart1"></div>
                  
                  <script>

                  var resultStaff = [];
                  var resultComp = [];

                    '<% for (var i=0, len=staffs.length; i<len; i++) { %>'
                      resultStaff.push('<%- staffs[i] %>');
                    '<% } %>'

                    '<% for (var i=0, len=compareStats.length; i<len; i++) { %>'
                      resultComp.push('<%- compareStats[i] %>');
                    '<% } %>'

                    document.addEventListener("DOMContentLoaded", () => {
                      new ApexCharts(document.querySelector("#comChart1"), {
                        series: [{
                          name: 'Average Marks',
                          data: resultComp
                        }],
                        chart: {
                          type: 'bar',
                          height: 600
                        },
                        plotOptions: {
                          bar: {
                            borderRadius: 4,
                            horizontal: true,
                          }
                        },
                        dataLabels: {
                          enabled: false
                        },
                        xaxis: {
                          categories: resultStaff,
                        }
                      }).render();
                    });
                  </script>
                  <!-- End Bar Chart -->
                </div>
              </div>
                </div>

    </div>
  </main><!-- End #main -->


  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  

  <!-- Vendor JS Files -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.4/jspdf.debug.js" ></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>

  <script src="https://cdn.jsdelivr.net/npm/canvg/dist/browser/canvg.min.js"></script>    
  <script src="https://cdnjs.cloudflare.com/ajax/libs/html2canvas/0.4.1/html2canvas.min.js"></script>    


  <script src="/vendor/apexcharts/apexcharts.min.js"></script>
  <script src="/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="/vendor/chart.js/chart.min.js"></script>
  <script src="/vendor/echarts/echarts.min.js"></script>
  <script src="/vendor/quill/quill.min.js"></script>
  <script src="/vendor/simple-datatables/simple-datatables.js"></script>
  <script src="/vendor/tinymce/tinymce.min.js"></script>
  <script src="/vendor/php-email-form/validate.js"></script>

  <!-- Template Main JS File -->
  <script src="/javascripts/main.js"></script>


</body>

</html>
<!DOCTYPE html>
<html lang="en">

<head>
  <title>Exit Feedbacks- FeedbackHUT</title>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="/images/favicon.png" rel="icon">
  <link href="/images/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link href="https://fonts.gstatic.com" rel="preconnect">
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="/vendor/quill/quill.snow.css" rel="stylesheet">
  <link href="/vendor/quill/quill.bubble.css" rel="stylesheet">
  <link href="/vendor/remixicon/remixicon.css" rel="stylesheet">
  <link href="/vendor/simple-datatables/style.css" rel="stylesheet">

  <!-- Template Main CSS File -->
  <link href="/stylesheets/style.css" rel="stylesheet">

</head>

<body>

  <!-- ======= Header ======= -->
  <%-include('../components/header')%>
  <!-- End Header -->

  <!-- ======= Sidebar ======= -->
  <%-include('../components/navbar')%>
  <main id="main" class="main">

    <div class="pagetitle">
      <h1>Exit Feedbacks (Analysis)</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/admin-feedback/dashboard">Home</a></li>
          <li class="breadcrumb-item">Feebacks</li>
          <li class="breadcrumb-item active">Exit Feedbacks</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section">
      <div class="row">

        <form action="/admin-feedback/analysis/exit" method="post" class="row g-3 needs-validation" novalidate>
          <div class="mb-3 col-md-3">
            <label for="yearpassing" class="form-label">Passing Year</label>
            <input type="text" class="form-control" name="yearpassing" placeholder="Passing Year" id="yearpass" required>
            <div class="valid-feedback">Looks good!</div>
            <div class="invalid-feedback">Please enter Passing Year.</div>
          </div>

          <div class="mb-3 col-md-3">
            <label for="department" class="form-label">Department</label>
            <select name="department" class="form-select" required>
                <option value="" selected>Choose Department</option>
                <option value="Computer Engineering">Computer Engineering</option>
                <option value="Information Technology">Information Technology</option>
                <option value="Electronics and Telecommunication">Electronics and Telecommunication</option>
                <option value="Mechanical Engineering">Mechanical Engineering</option>
                <option value="Electrical Engineering">Electrical Engineering</option>
            </select>
            <div class="valid-feedback">Looks good!</div>
            <div class="invalid-feedback">Please select Department.</div>
          </div>
          
          <div class="col-md-3">
            <button type="submit" class="btn btn-primary" style="margin:11%">Submit</button>
          </div>  
        </form>
      </div>
    </section>
    
    <%if(noents==true){%>
      <div class="text-center">
        <h5> No Data found!</h5>
      </div>
    <%}%>  

    <%if(!(avgsex && Object.keys(avgsex).length === 0 && Object.getPrototypeOf(avgsex) === Object.prototype) && noents==false) {%>
    
      <div class="row g-2">   
        <div class="card">
          
          <div class="row g-2">
            <div class="text-bold" style="margin: 1%;margin-top: 2%;">
              <b>Academic Feedbacks -</b>
            </div>
          </div> 

          <table width="100%" class="table">
            <thead>
              <tr>
                <th>Sr. No</th>
                <th>Particulars</th>
                <th>Average Marks</th>
            </tr>
            </thead>
            <tbody>
              
                <tr>
                    <td>1</td>
                    <td>Punctuality</td>
                    <td><%=avgsex.prepared%></td>
                </tr>

                <tr>
                  <td>2</td>
                  <td>Accessibility / Availability</td>
                  <td><%=avgsex.confidence%></td>
                </tr>

                <tr>
                  <td>3</td>
                  <td>Sincerity</td>
                  <td><%=avgsex.competencies%></td>
                </tr>
            </tbody>
          </table>


          <div class="row g-2">
            <div class="text-bold" style="margin: 1%;margin-top: 2;">
              <b>Administration and Facilities -</b>
            </div>
          </div> 

          <table width="100%" class="table">
            <thead>
              <tr>
                <th>Sr. No</th>
                <th>Particulars</th>
                <th>Average Marks</th>
            </tr>
            </thead>
            <tbody>
              
                <tr>
                    <td>1</td>
                    <td>Head of the department	</td>
                    <td><%=avgsex.hod%></td>
                </tr>

                <tr>
                  <td>2</td>
                  <td>Teaching Faculty</td>
                  <td><%=avgsex.faculty%></td>
                </tr>

                <tr>
                  <td>3</td>
                  <td>Non-teaching staff</td>
                  <td><%=avgsex.nonteaching%></td>
                </tr>
                
                <tr>
                  <td>4</td>
                  <td>Library</td>
                  <td><%=avgsex.library%></td>                         
                </tr>
              
                <tr>
                  <td>5</td>
                  <td>Laboratories</td>
                  <td><%=avgsex.laboratories%></td>
                </tr>

                <tr>
                  <td>6</td>
                  <td>Administration</td>
                  <td><%=avgsex.administration%></td>
                </tr>

                <tr>
                  <td>7</td>
                  <td>Training and Placement Officer	</td>
                  <td><%=avgsex.tpo%></td>                         
                </tr>

                <tr>
                  <td>8</td>
                  <td>Campus Placement opportunities	</td>
                  <td><%=avgsex.placement%></td>
                </tr>

                <tr>
                  <td>9</td>
                  <td>Discipline</td>
                  <td><%=avgsex.discipline%></td>                                                
                </tr>

                <tr>
                  <td>10</td>
                  <td>Overall environment	</td>
                  <td><%=avgsex.environment%></td>                      
                </tr>

                <tr>
                  <td>11</td>
                  <td>Canteen</td>
                  <td><%=avgsex.canteen%></td>                       
                </tr>

                <tr>
                  <td>12</td>
                  <td>Drinking water</td>
                  <td><%=avgsex.water%></td>                        
                </tr>

                <tr>
                  <td>13</td>
                  <td>Internet Facility	</td>
                  <td><%=avgsex.internet%></td>                          
                </tr>

                <tr>
                  <td>14</td>
                  <td>Cleanliness</td>
                  <td><%=avgsex.cleanliness%></td>                         
                </tr>

                <tr>
                  <td>15</td>
                  <td>Resolution of greviences	</td>
                  <td><%=avgsex.resolution%></td>                         
                </tr>

            </tbody>
          </table>
        </div>
      </div>
    <section class="section">
      <div class="row">
        


        <div class="col-lg-12">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Academic Feedback (Exit Feedbacks) </h5>

              <!-- Chart 1 -->
              <div id="barChart"></div>
              <script>
                document.addEventListener("DOMContentLoaded", () => {
                  new ApexCharts(document.querySelector("#barChart"), {
                    series: [{
                      name: 'Average Marks',
                      data: ['<%=avgsex.prepared%>', '<%=avgsex.confidence%>', '<%=avgsex.competencies%>']
                    }],
                    chart: {
                      type: 'bar',
                      height: 200
                    },
                    plotOptions: {
                      bar: {
                        borderRadius: 4,
                        horizontal: true,
                      }
                    },
                    dataLabels: {
                      enabled: false
                    },
                    xaxis: {
                      categories: ['Punctuality', 
                        'Accessibility / Availability',
                        'Sincerity'  
                      ],
                    }
                  }).render();
                });
              </script>

              <!-- Bar Chart 2 -->
              


              <h5 class="card-title">Administration and Facilities (Exit Feedbacks) </h5>
              <div id="barChart2"></div>

              <script>
                document.addEventListener("DOMContentLoaded", () => {
                  new ApexCharts(document.querySelector("#barChart2"), {
                    series: [{
                      name: 'Average Marks',
                      data: ['<%=avgsex.hod%>', '<%=avgsex.faculty%>', '<%=avgsex.nonteaching%>', '<%=avgsex.library%>', '<%=avgsex.laboratories%>', '<%=avgsex.administration%>', '<%=avgsex.tpo%>','<%=avgsex.placement%>', '<%=avgsex.discipline%>', '<%=avgsex.environment%>', '<%=avgsex.canteen%>', '<%=avgsex.water%>', '<%=avgsex.internet%>', '<%=avgsex.cleanliness%>','<%=avgsex.resolution%>']
                    }],
                    chart: {
                      type: 'bar',
                      height: 600
                    },
                    plotOptions: {
                      bar: {
                        borderRadius: 4,
                        horizontal: true,
                      }
                    },
                    dataLabels: {
                      enabled: false
                    },
                    xaxis: {
                      categories: ['Head of the department',
                      'Teaching Faculty',
                      'Non-teaching staff	',
                      'Library',
                      'Laboratories',
                      'Administration	',
                      'Training and Placement Officer	',
                      'Campus Placement opportunities	',
                      'Discipline	',
                      'Overall environment	',
                      'Canteen',
                      'Drinking water	',
                      'Internet Facility	',
                      'Cleanliness',
                      'Resolution of greviences'
                      ],
                    }
                  }).render();
                });
              </script>
              
              
            </div>
          </div>
        </div>

      </div>
    </section>
    <%}%>
  </main><!-- End #main -->


  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <!-- Vendor JS Files -->
  <script src="/vendor/apexcharts/apexcharts.min.js"></script>
  <script src="/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="/vendor/chart.js/chart.min.js"></script>
  <script src="/vendor/echarts/echarts.min.js"></script>
  <script src="/vendor/quill/quill.min.js"></script>
  <script src="/vendor/simple-datatables/simple-datatables.js"></script>
  <script src="/vendor/tinymce/tinymce.min.js"></script>
  <script src="/vendor/php-email-form/validate.js"></script>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.15.1/moment.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.7.14/js/bootstrap-datetimepicker.min.js"></script>


  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>



  <!-- Template Main JS File -->

  <script src="/javascripts/main.js"></script>


  

</body>

</html>